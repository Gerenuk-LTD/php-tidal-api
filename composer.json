{
  "name": "gerenuk/php-tidal-api",
  "description": "A PHP wrapper for Tidal's Api.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "tidal",
    "gerenuk",
    "tidal-api"
  ],
  "homepage": "https://github.com/Gerenuk-LTD/php-tidal-api",
  "authors": [
    {
      "name": "Kieran Proctor",
      "email": "kieran.proctor@geren.uk"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "gerenuk/php-coding-style": "^1.0",
    "laravel/pint": "^1.18",
    "phpstan/phpstan": "^2.0"
  },
  "repositories": {
    "gerenuk/php-coding-style": {
      "type": "vcs",
      "url": "https://github.com/Gerenuk-LTD/php-coding-style.git"
    }
  },
  "autoload": {
    "psr-4": {
      "TidalApi\\": "src/"
    }
  },
  "scripts": {
    "pint": "pint --config vendor/gerenuk/php-coding-style/pint.json"
  }
}
