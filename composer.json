{
  "name": "gerenuk/php-tidal-api",
  "description": "A PHP wrapper for Tidal's Api.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "tidal",
    "gerenuk",
    "tidal-api"
  ],
  "homepage": "https://github.com/Gerenuk-LTD/php-tidal-api",
  "authors": [
    {
      "name": "Kieran Proctor",
      "email": "kieran.proctor@geren.uk"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.5",
    "php-mock/php-mock-phpunit": "^2.7",
    "phpunit/phpunit": "^10.2",
    "squizlabs/php_codesniffer": "^3.0",
    "gerenuk/php-coding-style": "^1.0",
    "laravel/pint": "^1.18"
  },
  "repositories": {
    "gerenuk/php-coding-style": {
      "type": "vcs",
      "url": "https://github.com/Gerenuk-LTD/php-coding-style.git"
    }
  },
  "autoload": {
    "psr-4": {
      "TidalApi\\": "src/"
    }
  },
  "scripts": {
    "test": "phpcs src -v && phpunit"
  }
}
